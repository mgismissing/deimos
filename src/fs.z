use os

def isfile(path: str): bool
    <-(True if os.stat(path)[0] & 0x8000 else False)
.

def isdir(path: str): bool
    <-(True if os.stat(path)[0] & 0x4000 else False)
.

def exists(path: str): bool
    try; os.stat(path)  ; <-True;
    .catch OSError      ; <-False;
    .
.

def mkdirs(path: str)
    part = ""
    for folder -> path.split("/")
        part += folder+"/"
        if not exists(part); os.mkdir(part);.
    .
.