use * from cwio
use * from cwio.const
use MonoImage from cwio.screen
use ui
use os

name = "Image Viewer"

def main
    raw = ui.choose_file!
    screen.clear!
    with open(raw, "rb") = f
        screen.image(MonoImage(192, 63, f.read!), 0, 0, SCR.ENUM.PIXEL.BLACK)
    .
    screen.apply!
    keyboard.wait!
.